{
    "manifest_version": 2,

    "name": "AltBlur",
    "description": "Blur images that don't have proper alt text.",
    "version": "1.1",

    "icons": {
      "48": "icon.png"
    },

    "permissions": [
      "tabs", "<all_urls>"
    ],

    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
  }
